<table>
  <tbody>
  <tr>
    <th><%= l("available_custom_fields") %> :</th>
    <td>
      <%
         all_fields = CustomField.select("id, name").where(:type => "ProjectCustomField")
         options = []
         all_fields.each do |f|
           options << f.name
         end
      %>
      <%= select_tag "settings[custom_fields]", options_for_select(options, Setting.plugin_redmine_templates['custom_fields']), :id => "settings_custom_fields", :multiple => true, :size => 10 %>
    </td>
  </tr>
  </tbody>
</table>